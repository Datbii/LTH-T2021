    {
        static void Main(string[] args)
        {

            Console.OutputEncoding = Encoding.UTF8;
            Console.Write(" Nhập điểm:");
            float DTB = float.Parse(Console.ReadLine());
            if (DTB < 3.5)
            {
                Console.Write("Kém");
            }
            if (DTB < 5 & DTB >= 3.5)
            {
                Console.Write("Yếu");
            }
            if (DTB < 6.5 & DTB >= 5)
            {
                Console.Write("Trung Bình");
            }
            if (DTB < 8 & DTB >= 6.5)
            {
                Console.Write("Khá");
